package quiz;

// 인터페이스 정의
interface Animal{
	void sleep();
	void eat();
}

// 클래스 만들기
class LandAnimal{ // 육싱 동물
	void run() {
		System.out.println("육지 동물은 뛸 수 있습니다");
	}
}

class MarineAnimal{
	void swim() {
		System.out.println("해양 동물은 헤엄칠 수 있습니다.");
	}
}

class Dog extends LandAnimal implements Animal{

	@Override
	public void sleep() {
		System.out.println("개는 잠을 잡니다");
		
	}

	@Override
	public void eat() {
		System.out.println("개는 사료를 먹습니다.");
		
	}
}
	
class GoldFish extends MarineAnimal implements Animal{

	@Override
	public void sleep() {
		System.out.println("금붕어는 잠을 잡니다.");
		
	}

	@Override
	public void eat() {
		System.out.println("금붕어는 사료를 먹습니다.");
		
	}
}
	
class Tiger extends LandAnimal implements Animal{

	@Override
	public void sleep() {
		System.out.println("호랑이는 잡을 잡니다.");
		
	}

	@Override
	public void eat() {
		System.out.println("호랑이는 고기를 먹습니다.");
		
	}
}
	
class Shark extends MarineAnimal implements Animal{

	@Override
	public void sleep() {
		System.out.println("상어는 잠을 잡니다.");
		
	}

	@Override
	public void eat() {
		System.out.println("상어는 물고기를 먹습니다.");
	
	}
	
}

public class Bb {
	
	public static void main(String[] args) {
		Dog dog = new Dog();
		GoldFish goldFish = new GoldFish();
		Tiger tiger = new Tiger();
		Shark shark = new Shark();
		dog.sleep();
		dog.eat();
		goldFish.sleep();
		goldFish.eat();
		tiger.sleep();
		tiger.eat();
		shark.sleep();
		shark.eat();
	}

}
